{{#if this.challenge.previousChallenge}}
    {{!-- Previous challenge --}}
  <ButtonToChallenge id='previous-challenge-arrow' @challenge={{this.challenge.previousChallenge}}><RPaperIcon @iconName="keyboard_double_arrow_left"/></ButtonToChallenge>
{{/if}}

{{#if this.challenge.nextChallenge}}
    {{!-- Next challenge --}}
    <ButtonToChallenge id='next-challenge-arrow' @challenge={{this.challenge.nextChallenge}}><RPaperIcon @iconName="keyboard_double_arrow_right"/></ButtonToChallenge>
{{/if}}